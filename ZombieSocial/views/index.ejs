<% include _header %>
<h1> Bienvenid@ a la guarida </h1>
<a class="list-group-item list-group-item-primary">
Miembros Zombies
</a>
<% zombies.forEach((zombie)=> {%>
<div class="panel panel-default">
            <div class="panel-heading">
        <a href="/zombies/<%= zombie.username %>" class="list-group-item list-group-item-action">
            <%= zombie.name() %>
        </a>
    </div>
    <% if (zombie.bio) { %>
        <div class="panel-body"><%= zombie.bio %></div>
    <% } %>
    </div>
    <% }) %>

<% include _footer %>